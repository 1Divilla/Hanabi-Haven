{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "books": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book.book",
      "mappedBy": "users_permissions_user"
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter",
      "mappedBy": "users_permissions_user"
    },
    "comment_books": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment-book.comment-book",
      "mappedBy": "users_permissions_user"
    },
    "comment_chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment-chapter.comment-chapter",
      "mappedBy": "users_permissions_user"
    },
    "comment_replies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment-reply.comment-reply",
      "mappedBy": "users_permissions_user"
    },
    "histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::history.history",
      "mappedBy": "users_permissions_user"
    },
    "libraries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::library.library",
      "mappedBy": "users_permissions_user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "users_permissions_user"
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": false
    }
  }
}
